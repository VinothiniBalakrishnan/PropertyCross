<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @propertycross.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @propertycross.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="propertycross[test]" class="fieldLabel">Test</label>
            <input type="text" id="propertycross[test]" name="propertycross[test]" <%= placeholder("Test") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
