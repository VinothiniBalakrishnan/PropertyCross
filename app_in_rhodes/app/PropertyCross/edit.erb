<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @propertycross.test %></h1>
    <a href="<%= url_for :action => :show, :id => @propertycross.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @propertycross.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @propertycross.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="propertycross[test]" class="fieldLabel">Test</label>
            <input type="text" id="propertycross[test]" name="propertycross[test]" value="<%= @propertycross.test %>" <%= placeholder( "Test" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
